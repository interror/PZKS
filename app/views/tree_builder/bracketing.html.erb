<!doctype html>
<html>
    <head>
    </head>
    <body>
    	<h2 align="center"><%= button_to "Back", tree_builder_path, method: :get, params: { str: @string },  class: 'button' %></h2>
    	<h1 align="center">Input String: <%= @string %></h1>
    	<% counter = 1 %>
    	<% @out_result.each do |t| %>
            <h3 align="center"><p style="color: red;">Result <%= counter %></p> <%= t %></h3>
            <% counter += 1 %>
        <% end %>
    </body>
</html>